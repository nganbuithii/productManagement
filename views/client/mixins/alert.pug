mixin alert-success(time)
    - if (messages.info)
        .message.info
        div(Class="alert alert-success" data-time=time show-alert) #{messages.info} <span close-alert >X</span>

mixin alert-error(time)
    - if (messages.error)
        .message.info
        div(Class="alert alert-danger" data-time=time show-alert) #{messages.error} <span close-alert >X</span>
